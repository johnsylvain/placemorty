{
  "name": "placemorty",
  "version": "1.0.0",
  "author": "John Sylvain",
  "license": "MIT",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"./node_modules/.bin/babel-node src --presets es2015\"",
    "build": "./node_modules/.bin/babel src -s -D -d dist --presets es2015",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "./node_modules/.bin/eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "level": "^1.7.0",
    "node-cache": "^4.1.1",
    "pug": "^2.0.0-rc.2",
    "sharp": "^0.18.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "nodemon": "^1.11.0"
  }
}
